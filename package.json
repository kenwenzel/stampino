{
  "name": "stampino",
  "version": "0.6.2",
  "type": "module",
  "main": "./stampino.js",
  "exports": "./stampino.js",
  "files": [
    "stampino.{d.ts,js,js.map}",
    "src/*"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "tsc --build --clean",
    "test": "npx rollup --config rollup.tests.config.js && wtr build/test/**/*_test.js --node-resolve --playwright --browsers chromium",
    "test:watch": "wtr build/test/**/*_test.js --node-resolve --playwright --browsers chromium --watch",
    "benchmark": "tach --config tachometer.config.json",
    "format": "prettier \"src/**/*.ts\" --write",
    "demo": "wds --node-resolve"
  },
  "dependencies": {
    "jexpr": "^1.0.0-pre.2",
    "lit-html": "^2.0.0",
    "trusted-types": "^2.0.0"
  },
  "devDependencies": {
    "@esm-bundle/chai": "^4.3.4",
    "@rdfjs/types": "^1.1.0",
    "@rollup/plugin-commonjs": "^22.0.0-13",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@types/mocha": "^8.2.2",
    "@types/n3": "^1.10.4",
    "@web/dev-server": "^0.1.5",
    "@web/dev-server-rollup": "^0.3.2",
    "@web/test-runner": "^0.13.3",
    "@web/test-runner-mocha": "^0.7.2",
    "@web/test-runner-playwright": "^0.8.4",
    "n3": "^1.14.0",
    "prettier": "^2.2.1",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-replace": "^2.2.0",
    "sparql-engine": "^0.8.2",
    "tachometer": "^0.5.8",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.0",
    "webpack": "^5.69.1"
  },
  "keywords": [
    "html",
    "lit-html",
    "template"
  ],
  "author": "Justin Fagnani <justin@fagnani.com>",
  "license": "MIT",
  "repository": "justinfagnani/stampino",
  "bugs": {
    "url": "https://github.com/justinfagnani/stampino/issues"
  },
  "homepage": "https://github.com/justinfagnani/stampino#readme"
}
